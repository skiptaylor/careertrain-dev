	<div class="container">	
		<div class="row">  
			<div class="col-md-12">
				<h1 class="page-header">New Recruiter</h1>
			</div>
		</div>
		
		<form method="post">
					
		<div class="form-row">
			<div class="form-group col-md-6">
				<label for="email">Email</label>
				<input type="text" name="email" id="email" class="form-control" value="<%= @recruiter.email %>" />
			</div>
			<div class="form-group col-md-6">
			<label for="password">Password</label>
				<input type="text" name="password" id="password" class="form-control" value="<%= @recruiter.password %>" />
			</div>
		</div>	
		<div class="form-row">
			<div class="form-group">
				<label class="checkbox">&nbsp;</label>
				<input type="checkbox" name="recruiter_status" id="recruiter_status" <%= 'checked="checked"' if @recruiter.recruiter_status  %> /><strong>&nbsp;&nbsp;&nbsp;RRC</strong><label class="checkbox">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
 				<input type="checkbox" name="active" id="active" <%= 'checked="checked"' if @recruiter.active  %> />&nbsp;&nbsp;&nbsp;<strong>Active Recruiter</strong>
				
			</div>
		</div>		
		<div class="form-row">
			<div class="form-group col-md-1">
				<label for="rank">Rank</label>
				<input type="text" name="rank" id="rank" class="form-control" value="<%= @recruiter.rank %>" />
			</div>
			<div class="form-group col-md-4">
				<label for="first_name">Fist name</label>
				<input type="text" name="first_name" id="first_name" class="form-control" value="<%= @recruiter.first_name %>" />
			</div>
			<div class="form-group col-md-3">
				<label for="middle_name">Middle name</label>
				<input type="text" name="middle_name" id="middle_name" class="form-control" value="<%= @recruiter.middle_name %>" />
			</div>
			<div class="form-group col-md-4">
				<label for="last_name">Last name</label>
				<input type="text" name="last_name" id="last_name" class="form-control" value="<%= @recruiter.last_name %>" />
			</div>
		</div>
		
		<div class="form-row">
			<div class="form-group col-md-6">
				<label for="address1">Address</label>
				<input type="text" name="address1" id="address1" class="form-control" value="<%= @recruiter.address1 %>" />
			</div>
			<div class="form-group col-md-6">
				<label for="address2">&nbsp;</label>
				<input type="text" name="address2" id="address2" class="form-control" value="<%= @recruiter.address2 %>" />
			</div>
		</div>
		<div class="form-row">
			<div class="form-group col-md-6">
				<label for="city">City</label>
				<input type="text" name="city" id="city" class="form-control" value="<%= @recruiter.city %>" />
			</div>
			<div class="control-group col-md-2">
				<label for="state">State</label>
				<select type="text" class="form-control" id="state" name="state" value="<%= @recruiter.state %>">
						<option value=""></option>
					<% @state.sort{|a,b| a.abbr <=> b.abbr}.each do |state| %>
						<option value="<%= state.abbr %>" <%= 'selected' if state.abbr == @recruiter.state %>><%= state.abbr %></option>
					<% end %>
				</select>
			</div>
			<div class="control-group col-md-3">
				<label for="zip">ZIP</label>
				<input type="text" name="zip" id="zip" class="form-control" value="<%= @recruiter.zip %>" />
			</div>
		</div>
		<div class="form-row">
			<div class="form-group col-md-6">
				<label for="phone">Phone</label>
				<input type="text" name="phone" id="phone" class="form-control" value="<%= @recruiter.phone %>" />
			</div>
			<div class="form-group col-md-3">
				<label for="ssnl4">SSN last 4 digits only</label>
				<input type="text" name="ssnl4" id="ssnl4" class="form-control" value="<%= @recruiter.ssnl4 %>" />
			</div>
		</div>
				
				<!-- <div class="control-group">
				    <div class="controls">
				      <label class="checkbox">
	 			    	  <input type="checkbox" name="recruiter_status" id="recruiter_status" <%= 'checked="checked"' if @recruiter.recruiter_status  %> />
						  <strong>Active</strong>
	 			  		</label>
					</div>
				</div> -->
				
		<div class="form-row">
			<div class="form-group col-md-12">
				<a href="/arng/arng/" class="btn btn-secondary btn-small pull-right">Cancel</a><input class="btn btn-small btn-primary pull-right" style="margin-right: 24px;" type="submit" value="Save" /> &nbsp; 
			</div>
			</form>	
			
		</div> <!-- /row -->
	</div> <!-- /container -->
