<div class="container-white">
	<div class="container">	
		<div class="row">  
			<div class="span10 offset1">
				<p class="pull-right"><a href="/recruiters/recruiters/" class="btn btn-small">Cancel</a>&nbsp;&nbsp;<a href="/recruiters/<%= @recruiter.id %>/edit/" class="btn btn-small">Edit</a></p>
				<h1>Recruiter</h1>
			</div>	
			<div class="span5 offset1">
				<p>
					<%= @recruiter.rank %> <%= @recruiter.first_name %> <%= @recruiter.middle_name %> <%= @recruiter.last_name %><br />
					<% if @recruiter.recruiter_status == true %>RRC<% end %>
				</p>
				<p>
					<%= @recruiter.email %>
				</p>
			</div>
			<div class="span5">
				<p>
					<%= @recruiter.address1 %><br />
					<% unless @recruiter.address2 = "" %><%= @recruiter.address2 %><br /><% end %>
					<%= @recruiter.city %> <%= @recruiter.state %> &nbsp;<%= @recruiter.zip %><br />
				</p>
				<p>
					<%= @recruiter.phone %><br />
					SSN ... <%= @recruiter.ssnl4 %>
				</p>	
			</div>
		</div>
	</div>
</div>