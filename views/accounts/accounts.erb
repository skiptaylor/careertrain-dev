<div class="white">
	<div class="container">
		<div class="row" style="padding: 70px 0 24px 0;">
			<div class="col-md-8 offset2">
			<h5>State Accounts</h5>
			<table class="table table-striped table-condensed">
				<tr>
				<th> </th>	
				<th> </th>
				<th>State</th>
				<th><a href="/accounts/new" class="btn btn-mini btn-success pull-right"><i class="icon icon-plus"></i>  New</a></th>
			</tr>
			<% @account.each do |account| %>
			<tr>
				<td style="color: #b4c7ce;"><small><%= account.id %></small></td>
				<td><strong><%= account.abbr %></strong></td>
				<td><%= account.name %></td>
				<td style="text-align: right;"><a href="/accounts/<%= account.id %>/edit" class="btn btn-mini">Edit</a></td>
			</tr>
			<% end %>
			</table>
		</div>
	</div>
</div>
