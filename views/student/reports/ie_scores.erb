<div class="container-white">
	<div class="container">
		<div class="row" style="padding: 20px 0 24px 0;">
			<div class="span12">
				<a href="/student/reports/enter_score" class="btn btn-small pull-right">Back</a>
				<h1>Career Direction Interest Exercise</h1>
				<hr />
			</div>
			<form method="post" id="exercise">
			<div class="span8 offset2">
			<table class="table table-striped table-condensed">
				<h3>Your Career Direction Interest Exercise Scores</h3>
					
			<% Exercise.reverse.each do |e| %>
				<tr>
					<td style="text-align: left;"><%= format_day(e.created_at) %></td>
					<td style="text-align: center;">A - <%= e.score_a %></td>
					<td style="text-align: center;">B - <%= e.score_b %></td>
					<td style="text-align: center;">C - <%= e.score_c %></td>
					<td style="text-align: center;">D - <%= e.score_d %></td>
					<td style="text-align: center;">E - <%= e.score_e %></td>
					<td style="text-align: center;">F - <%= e.score_f %></td>
					<td style="text-align: center;">G - <%= e.score_g %></td>
					<td style="text-align: center;">H - <%= e.score_h %></td>
					<td style="text-align: center;">I - <%= e.score_i %></td>
				</tr>
			<% end %>
		</table>
 			
			</div>
			</form>
			
		</div>
	</div>
</div>