	<div class="container">	
		<div class="row">  
			<div class="col-md-12">
				<a href="/admin/admin_edit" class="btn btn-small pull-right" style="margin-top:12px;">Admin Index</a>
				<h1 class="page-header">Admins</h1>
			</div>
		</div>
		<div class="row">
			<div class="col-md-8 offset-md-2">
			<table class="table table-striped table-condensed">
				<tr>
					<th>Name</th>	
					<th><a href="/admin/new" class="btn btn-mini btn-success pull-right"><i class="icon icon-plus"></i>  New</a></th>
				</tr>
			<% @admin.each do |a| %>
				<tr>
					<td><%= a.first_name %> <%= a.last_name %></td>
					<td style="text-align: right;"><a href="/admin/<%= a.id %>/edit" class="btn btn-mini">Edit</a></td>
				</tr>
			<% end %>
			</table>
		</div>
	</div>
</div>